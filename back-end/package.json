{
  "name": "homework",
  "version": "1.0.0",
  "scripts": {
    "dist": "tsc -p .",
    "dev": "tsnd --respawn index.ts",
    "install-postgresql": "bash scripts/install-postgresql-linux.sh",
    "install-postgresql-mac": "bash scripts/install-postgresql-mac.sh",
    "db-migrate": "cd dist && npx sequelize db:migrate",
    "db-migrate-undo": "npx sequelize db:migrate:undo",
    "db-migrate-undo-all": "cd dist && npx sequelize db:migrate:undo:all",
    "db-seed-data": "cd dist && npx sequelize db:seed:all",
    "start-api": "tsnd scripts/api.ts &",
    "start-frontend-dev": "cd ../front-end && npm start",
    "start-install-db": "npm run install-postgresql-mac && npm start",
    "start": "npm run db-migrate && npm run db-seed-data && npm run start-api"
  },
  "dependencies": {
    "@types/express": "^4.17.18",
    "@types/luxon": "^3.3.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "luxon": "^3.4.3",
    "pdfdataextract": "^3.2.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.33.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.4",
    "@types/pdf-parse": "^1.1.2",
    "@types/validator": "^13.11.2",
    "sequelize-cli": "^6.6.1",
    "sequelize-typescript": "^2.1.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
